[
  {
    "type": "MOD_INFO",
    "id": "mom_channeling_quadruple_experience",
    "name": "Mind Over Matter: <color_magenta>Quadrupled Experience</color>",
    "authors": [ "Standing-Storm" ],
    "description": "Quadruples the experience gains when channeling or contemplating powers.",
    "category": "misc_additions",
    "dependencies": [ "dda", "mindovermatter" ],
    "conflicts": [ "mom_channeling_ten_times_experience", "mom_channeling_double_experience" ]
  },
  {
    "type": "jmath_function",
    "id": "psionic_power_experience_formula_channeling",
    "num_args": 0,
    "return": "(u_val('focus_effective') / 100 ) * 320 * ( ( u_val('intelligence') + 10) / 20 ) * min( ( 0.5 + ( u_skill('metaphysics') / 20) ), 1) * u_nether_attunement_power_scaling"
  },
  {
    "type": "jmath_function",
    "id": "psionic_power_experience_formula",
    "num_args": 0,
    "return": "(u_val('focus_effective') / 100) * 320 * ( ( u_val('intelligence') + 10) / 20 ) * min( ( 0.5 + ( u_skill('metaphysics') / 20) ), 1) * u_nether_attunement_power_scaling"
  },
  {
    "type": "jmath_function",
    "id": "contemplation_factor",
    "num_args": 0,
    "return": "(u_val('focus_effective') * 0.004) * u_nether_attunement_power_scaling * clamp( ( ( u_vitamin('vitamin_maintained_powers') > 0 ) ? ( (1 + u_has_proficiency('prof_concentration_master') ) / ( 1 + u_vitamin('vitamin_maintained_powers') ) ) : 1), 0.1,1) * 4"
  }
]
