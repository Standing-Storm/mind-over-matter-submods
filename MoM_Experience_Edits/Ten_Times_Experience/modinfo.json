[
  {
    "type": "MOD_INFO",
    "id": "mom_channeling_ten_times_experience",
    "name": "Mind Over Matter: <color_magenta>Ten Times Experience</color>",
    "authors": [ "Standing-Storm" ],
    "description": "Multiplies by ten the experience gains when channeling or contemplating powers.",
    "category": "misc_additions",
    "dependencies": [ "dda", "mindovermatter" ],
    "conflicts": [ "mom_channeling_double_experience", "mom_channeling_quadruple_experience" ]
  },
  {
    "type": "jmath_function",
    "id": "psionic_power_experience_formula_channeling",
    "num_args": 0,
    "return": "(u_val('focus_effective') / 100 ) * 800 * ( ( u_val('intelligence') + 10) / 20 ) * min( ( 0.5 + ( u_skill('metaphysics') / 20) ), 1) * u_nether_attunement_power_scaling"
  },
  {
    "type": "jmath_function",
    "id": "psionic_power_experience_formula",
    "num_args": 0,
    "return": "(u_val('focus_effective') / 100) * 800 * ( ( u_val('intelligence') + 10) / 20 ) * min( ( 0.5 + ( u_skill('metaphysics') / 20) ), 1) * u_nether_attunement_power_scaling"
  }
]
